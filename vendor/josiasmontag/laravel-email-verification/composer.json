{
  "name": "josiasmontag/laravel-email-verification",
  "description": "Laravel Email Verification",
  "license": "MIT",
  "authors": [
    {
      "name": "Josias Montag",
      "email": "josias@montag.info"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
    "illuminate/queue": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
    "illuminate/auth": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
    "nesbot/carbon": "^1.20|2.0"
  },
  "require-dev": {
    "mockery/mockery": "1.2",
    "phpunit/phpunit": "^5.7|6.2|^7.0",
    "orchestra/testbench": "~3.4.0|^3.5.0|^3.6.0|^3.7.0|^3.8.0",
    "laravel/framework": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
  },
  "autoload": {
    "psr-4": {
      "Lunaweb\\EmailVerification\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lunaweb\\EmailVerification\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "Lunaweb\\EmailVerification\\Providers\\EmailVerificationServiceProvider"
      ]
    }
  }
}
